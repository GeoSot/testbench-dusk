{
    "name": "orchestra/testbench-dusk",
    "description": "Laravel Dusk Testing Helper for Packages Development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keoghan Litchfield",
            "email": "keoghan@klever.co.uk",
            "homepage": "https://klever.co.uk"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testbench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Testbench\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "jeremeamia/superclosure": "^2.3",
        "laravel/dusk": "~1.0.15",
        "orchestra/testbench": "~3.4"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.7"
    },
    "suggest": {
        "orchestra/database": "Allow to use --realpath migration for testing (~3.4)."
    },
    "config": {
        "sort-packages": true
    }
}
